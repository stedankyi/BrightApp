        <Authorized>
            <MudNavLink Href="/profile" Disabled="true" Match="NavLinkMatch.All" Icon="@Icons.Material.Rounded.AccountCircle">
                <MudText Typo="Typo.h6">
                    Profile
                </MudText>
            </MudNavLink>
